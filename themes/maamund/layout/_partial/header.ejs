<header id="header">
  <!-- NavBar toggler: burguer -->
  <button class="hamburger hamburger--squeeze align-self-center" type="button" data-toggle="collapse" data-target="#navbarToggler" aria-controls="navbarToggler" aria-expanded="false" aria-label="Toggle navigation" tabindex="0">
      <span class="hamburger-box">
		    <span class="hamburger-inner"></span>
			</span>
  </button>
	<nav class="container-fluid navbar d-flex flex-row justify-content-between align-items-start">	  
	  <!-- Nav: Link #1 -->
		<%- partial('_partial/menu', {filter: 1}) %>
		<!-- Brand logo -->
	  <a class="navbar-brand align-self-start" href="<%- url_for() %>" title="<%= config.title %>">
	  	<span><%= config.subtitle %></span>
	  	<%= config.title %>
	  </a>
	  <!-- Opaque background -->
		<div class="bg-nav"></div>
	</nav>
	<!-- Collapsed menu activated with the burguer link -->
	<%- partial('_partial/menu_collapsed', {id: 'navbarToggler'}) %>
</header>
